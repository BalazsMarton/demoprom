<div class="parallax-container valign-wrapper" id="contactus">
    <div class="section no-pad-bot">

      <div class="container">
      <div class="row">
    <div class="col s10 m10 l8 offset-s1 offset-m1 offset-l2">
    <div class="row" id="parallax-form">

    <h5 class="col s12 center grey-text text-lighten-4">WE'D LOVE TO WORK WITH YOU</h5>    


        <div class="input-field col s12 m6 l6">
        <i class="material-icons prefix">account_circle</i>
          <input id="first_name" type="text" class="validate" required>
          <label for="first_name"  data-error="wrong">YOUR FIRST NAME</label>
        </div>

        <div class="input-field col s12 m6 l6">
        <i class="material-icons prefix">account_circle</i>
          <input id="last_name" type="text" class="validate" required>
          <label for="last_name"  data-error="wrong">YOUR LAST NAME</label>
        </div>


        <div class="input-field col s12 m6 l6">
        <i class="material-icons prefix">phone</i>
          <input id="icon_telephone" type="tel" class="validate" required>
          <label for="icon_telephone"  data-error="wrong">YOUR PHONE NOUMBER</label>
        </div>
      



        <div class="input-field col s12 m6 l6">
        <i class="material-icons prefix">email</i>
          <input id="email" type="email" class="validate" required>
          <label for="email"  data-error="wrong">YOUR EMAIL ADRESS</label>
        </div>

      
      
    <form class="col s12">
      <div class="row">
        <div class="input-field col s12">
          <i class="material-icons prefix">mode_edit</i>
          <textarea id="icon_prefix2" class="materialize-textarea"></textarea>
          <label for="icon_prefix2">YOUR MESSAGE</label>
        </div>
      </div>
    </form>
    <div class="input-field col s12 center">
      <button class="btn waves-effect waves-light" type="submit" name="action">SEND
      <i class="material-icons right">send</i>
      </button>
    </div>

  </div>
  </div>
  </div>

  </div>

    </div>
    <div class="parallax"><img src="img/contactusbg.jpg" alt="Unsplashed background img 1"></div>
  </div>